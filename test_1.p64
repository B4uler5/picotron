--[[pod_format="raw",created="2024-05-02 22:07:57",modified="2024-05-03 09:08:03",revision=135]]
function _init()
	pressed_for = 0
	window(240,135)
	cls(14)
end

function _draw()
	mx,my,mb = mouse()
	if mb == 1 then
		pressed_for += 1
		for i=1,24 do
			circ(mx,my, i*(pressed_for*0.01), i)
		end
	end
	if mb == 0 then
		pressed_for = 0
	end
end


